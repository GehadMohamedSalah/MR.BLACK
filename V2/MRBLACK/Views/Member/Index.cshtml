@model MRBLACK.Helper.PagedList<MRBLACK.Models.ViewModels.MemberVM>
@using MRBLACK.Models.ViewModels;
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var paginationVM = new PaginationVM()
    {
        PageIndex = Model.PageIndex,
        TotalPages = Model.TotalPages,
        PreviousPage = Model.PreviousPage,
        NextPage = Model.NextPage,
        GetItemsUrl = "/Member/GetItems",
        GetPaginationUrl = "/Member/GetPagination",
        ItemsCount = Model.ItemsCount,
        StartIndex = Model.StartIndex,
        EndIndex = Model.EndIndex
    };
}

<button type="button" class="btn btn-buy" onclick="BackToPreviousPage()"><i class="fa fa-forward"></i> العودة للصفحة السابقة</button>

<h1 class="text-center">شاشة الاعضاء</h1>
<div class="panel panel-default">
    <div class="panel-heading">
        <div class="row">
            <div class="col-sm-6">
            </div>
            <partial name="_TableNavbar" model="@Model.PageIndex" />
        </div>



    </div>
    <div class="panel-body">
        <div class="table-responsive">
            <table class="table table-striped table-hover table-bordered">
                <thead class="bg-blue">
                    <tr>
                        <td>#</td>
                        <td>الكود</td>
                        <td>الاسم</td>
                        <td>رقم العضوية</td>
                        <td>البريد الالكتروني</td>
                        <td>رقم الهاتف</td>
                        <td>نوع الباقة</td>
                        <td>الجنس</td>
                        <td>الرصيد</td>
                    </tr>
                </thead>
                <tbody id="tbBody">
                    <partial name="_TableList" model="@Model.ToList()" />
                </tbody>
            </table>
            <div id="dvPagination">
                <partial name="_Pagination" model="@paginationVM" />
            </div>
        </div>

    </div>
</div>

